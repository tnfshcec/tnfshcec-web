{
  "name": "tnfshcec-web",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "npm run build:i18n && vite build",
    "build:i18n": "paraglide-js compile --project ./tnfshcec.inlang",
    "preview": "vite preview",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint . && npx @inlang/cli lint --project ./tnfshcec.inlang",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@iconify-json/mdi": "^1.1.54",
    "@inlang/paraglide-js": "1.3.5",
    "@inlang/paraglide-js-adapter-sveltekit": "^0.5.5",
    "@melt-ui/pp": "^0.3.0",
    "@melt-ui/svelte": "^0.76.2",
    "@playwright/test": "^1.42.1",
    "@shikijs/transformers": "^1.2.3",
    "@shikijs/twoslash": "^1.2.3",
    "@sveltejs/adapter-static": "^3.0.1",
    "@sveltejs/kit": "^2.3.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@tailwindcss/forms": "^0.5.4",
    "@tailwindcss/typography": "^0.5.9",
    "@types/node": "^20.12.2",
    "@types/thunky": "^1.1.0",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "autoprefixer": "^10.4.14",
    "bits-ui": "^0.21.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-svelte": "^2.35.0",
    "mdsvex": "^0.11.0",
    "mode-watcher": "^0.3.0",
    "postcss": "^8.4.23",
    "postcss-load-config": "^5.0.2",
    "prettier": "^3.2.4",
    "prettier-plugin-svelte": "^3.2.2",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "prism-themes": "^1.9.0",
    "rehype-external-links": "^3.0.0",
    "rehype-slug": "^6.0.0",
    "remark-footnotes": "^4.0.1",
    "shiki": "^1.1.0",
    "svelte": "^4.2.3",
    "svelte-check": "^3.6.0",
    "svelte-meta-tags": "^3.1.0",
    "svelte-preprocess": "^5.0.3",
    "svelte-sequential-preprocessor": "^2.0.1",
    "tailwindcss": "^3.3.1",
    "tslib": "^2.4.1",
    "typescript": "^5.4.3",
    "unist-util-visit": "^5.0.0",
    "unplugin-icons": "^0.18.1",
    "vite": "^5.2.7"
  },
  "type": "module"
}
