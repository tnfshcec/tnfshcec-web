{
  "name": "tnfshcec-web",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "npm run build:i18n && vite build",
    "build:i18n": "paraglide-js compile --project ./tnfshcec.inlang",
    "preview": "vite preview",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint . && npx @inlang/cli lint --project ./tnfshcec.inlang",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@iconify-json/mdi": "^1.1.54",
    "@inlang/paraglide-sveltekit": "^0.16.0",
    "@melt-ui/pp": "^0.3.0",
    "@melt-ui/svelte": "^0.86.0",
    "@playwright/test": "^1.50.1",
    "@shikijs/transformers": "^3.1.0",
    "@sveltejs/adapter-static": "^3.0.1",
    "@sveltejs/kit": "^2.17.3",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@tailwindcss/typography": "^0.5.9",
    "@types/eslint": "^9.6.0",
    "@types/node": "^22.13.8",
    "autoprefixer": "^10.4.14",
    "bits-ui": "^1.3.5",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-svelte": "^3.0.2",
    "mdsvex": "^0.12.3",
    "mode-watcher": "^0.5.1",
    "postcss": "^8.4.23",
    "postcss-load-config": "^6.0.1",
    "prettier": "^3.5.2",
    "prettier-plugin-svelte": "^3.2.6",
    "prettier-plugin-tailwindcss": "^0.6.4",
    "rehype-external-links": "^3.0.0",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.0",
    "shiki": "^3.1.0",
    "svelte": "^5.20.5",
    "svelte-check": "^4.0.0",
    "svelte-meta-tags": "^4.1.0",
    "svelte-preprocess": "^6.0.1",
    "svelte-sequential-preprocessor": "^2.0.1",
    "tailwindcss": "^4.0.9",
    "tslib": "^2.4.1",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.25.0",
    "unist-util-visit": "^5.0.0",
    "unplugin-icons": "^22.1.0",
    "vaul-svelte": "^1.0.0-next.3",
    "vite": "^6.2.0"
